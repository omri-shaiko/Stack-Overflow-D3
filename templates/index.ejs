<!doctype html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>StackOverflow D3</title>
  <link rel="stylesheet" href="bower_components/bootstrap/dist/css/bootstrap.min.css"/>
</head>
<body ng-app="stackD3">

<div class="container" ng-controller="UsersCtrl">
  <header>
    <h3 class="text-muted">StackOverflow D3</h3>
  </header>

  <div class="jumbotron">
    <h1>StackOverflow D3</h1>


    <form class="form-inline" ng-submit="searchUser(userName)">
      <div class="form-group">
        <input type="text" class="form-control" ng-model="userName" placeholder="Please enter user name" />
      </div>
      <button type="submit" class="btn btn-default">Search User</button>
    </form>
  </div>

</div> <!-- /container -->

<script src="bower_components/angularjs/angular.min.js"></script>
<script src="bower_components/d3/d3.min.js"></script>
<script src="app.js"></script>
</body>
</html>
